{
  "name": "re-animator",
  "version": "1.0.2",
  "description": "Simple animator using CSS3 animations",
  "main": "lib/index.js",
  "jsnext:main": "lib/index.js",
  "homepage": "https://github.com/mathieuancelin/re-animator",
  "scripts": {
    "lint": "eslint src",
    "start": "NODE_ENV=dev webpack-dev-server -d --colors --hot --inline --progress",
    "lib": "babel src --out-dir lib",
    "bundle": "NODE_ENV=production webpack -p --colors --progress",
    "build": "npm run lint && npm run lib && npm run bundle"
  },
  "keywords": [
    "CSS3",
    "animation",
    "simple"
  ],
  "author": "Mathieu ANCELIN",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^5.8.25"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-loader": "^6.1.0",
    "eslint": "^1.9.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.8.0",
    "webpack": "^1.12.4",
    "webpack-dev-server": "^1.12.1"
  }
}
